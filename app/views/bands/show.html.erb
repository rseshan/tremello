<p id="notice"><%= notice %></p>

<p>
  <b>Name:</b>
  <%= @band.name %>
</p>

<p>
  <b>Bio:</b>
  <%= @band.bio %>
</p>

<p>
  <b>Members:</b>
  <ul>
    <% @band.musicians.each do |member| %>
      <li><%= link_to member.name, member %></li>
    <% end %>
  </ul>
</p>

<%= link_to 'Edit', edit_band_path(@band) %> |
<%= link_to 'Back', bands_path %>
